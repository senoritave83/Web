/************************************* IMAGE ROTATOR **************************************************/

.ImageRotator {text-align:center; padding:1px ; width:102px; border-left:1px solid #C7D1DC; border-right:1px solid #C7D1DC;}
.ImageRotator .ImageRotatorTable {height:15px;border: none 0px; padding:0px 0px 0px 0px;width:102px; width:100px ; top:0px;position:relative;font-family:Arial;color:White;filter:alpha(opacity=50);-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5; z-index:1;}
.ImageRotator .ImageRotatorTable tr {height:15px;}
.ImageRotator .ImageRototorImg {width:100px;}
.ImageRotator .tdSeparator {width:3px;}
.ImageRotator .tdButton {background-color:#617E93;width:20px; border:1px #FFFFFF solid; border-left:none ; border-top:none ; border-bottom:none ; padding:0px;}
.ImageRotator .tdBtRev {background-color:#617E93;width:18px; width:20px ; border:1px #FFFFFF solid; border-left:none ; border-top:none ; border-bottom:none ; background-image:url('../../imagens/player_rev.png');}
.ImageRotator .tdBtRev:hover { background-image:url('../../imagens/player_rev_over.png');}

.ImageRotator .tdCaption {background-color:#617E93; border:1px solid #FFFFFF; border-left:none ; border-top:none ; border-bottom:none ; padding:0px;}
.ImageRotator .tdCaption span {color:#FFFFFF;}
    
.ControlPergunta .ImageRotator {background-color:Black;text-align:center;padding:1px 1px 1px 1px;width:502px;}
.ControlPergunta .ImageRotator .ImageRotatorTable {height:15px;border: none 0px; padding:0px 0px 0px 0px;width:500px;font-family:Arial;color:White;filter:alpha(opacity=50);-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5; z-index:1;}
.ControlPergunta .ImageRotator .ImageRotatorTable tr {height:15px;}
.ControlPergunta .ImageRotator .ImageRototorImg {width:500px;}
.ControlPergunta .ImageRotator .tdSeparator {width:3px;}
.ControlPergunta .ImageRotator .tdButton {background-color:black;width:15px;height:15px;padding:0px 0px 0px 0px;}
.ControlPergunta .ImageRotator .tdCaption {background-color:Gray;}
   
/******************************************* IMAGE UPLOADER ****************************************************/

.ImageUploader {
	PADDING-BOTTOM: 1px; PADDING-LEFT: 1px; WIDTH: 171px; PADDING-RIGHT: 10px; MARGIN-BOTTOM: 10px; MARGIN-LEFT: 29px; PADDING-TOP: 1px;float:left;
}
.ImageUploader .normal {
	FILTER: alpha(opacity=100); WIDTH: 150px; opacity: 1;
	border:2px #AABDD4 solid;
