#divFormularioProdutos table thead tr .thFornecedor {background-color:#CCCCCC;white-space:nowrap;color:black;}
#divFormularioProdutos table thead tr .thSubCategoria {background-color:#CCCCCC;white-space:nowrap;color:black;}

#divFormularioProdutos table tbody .trAlternate td {background-color:white;white-space:nowrap;color:black;vertical-align:middle;}
#divFormularioProdutos table tbody .trAlternate .tdProduto {background-color:white;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trAlternate .tdStatus {background-color:white;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trAlternate .tdFornecedor {background-color:white;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trAlternate .tdSubCategoria {background-color:white;white-space:nowrap;color:black;}

#divFormularioProdutos table tbody .trNormal td {background-color:#CCCCCC;white-space:nowrap;color:black;vertical-align:middle;}
#divFormularioProdutos table tbody .trNormal .tdProduto {background-color:#CCCCCC;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trNormal .tdStatus {background-color:#CCCCCC;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trNormal .tdFornecedor {background-color:#CCCCCC;white-space:nowrap;color:black;}
#divFormularioProdutos table tbody .trNormal .tdSubCategoria {background-color:#CCCCCC;white-space:nowrap;color:black;}

#divFormularioProdutos table tfoot tr td {background-color:#CCCCCC;white-space:nowrap;color:white;}

#divFormularioProdutos table tbody tr .td10 {background-color:#CCCCCC;}
