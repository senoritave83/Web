<%@ Page Language="vb" AutoEventWireup="false" Codebehind="Default.aspx.vb" Inherits="xmlinkwm._Default" %>
<%@ Register TagPrefix="uc1" TagName="inc_menu" Src="inc/inc_menu.ascx" %>
<HTML>
<!-- #INCLUDE FILE='inc/inc_header.ascx' -->
<body MS_POSITIONING="GridLayout" topmargin="0" leftmargin="0" rightmargin="0" bottommargin="0">
	<!-- #INCLUDE FILE='inc/inc_top.ascx' -->
	<table width="100%" cellpadding="0" cellspacing="0" height="100%">
		<tr>
			<td>
				<uc1:inc_menu id="Inc_menu1" runat="server"></uc1:inc_menu>
			</td>
			<td class='BackgrStripes' rowspan="2">&nbsp;</td>
		</tr>
		<tr height="100%">
			<td width="750" valign="top" align="middle">
				<!-- INICIO CONTEUDO -->
				<form id="Form1" method="post" runat="server">
					<table border="0" cellspacing="4" cellpadding="2" width="740" height="100%">
						<tr vAlign="middle" height="60">
							<td>
							</td>
						</tr>
						<tr>
							<td>
								<table border="0">
									<tr>
										<td class="Banner">
											<table width="100%" cellpadding="4" cellspacing="4">
												<tr>
													<td colspan="3">
														<b class="Banner" style="Font-size:14pt">Seus Vendedores, e suas atividades, na 
															tela do seu computador.</b>
													</td>
												</tr>
												<tr>
													<td><b>Simples.</b></td>
													<td colspan="2">&nbsp;</td>
												</tr>
												<tr>
													<td>&nbsp;</td>
													<td><b>Rápido.</b></td>
													<td>&nbsp;</td>
												</tr>
												<tr>
													<td colspan="2">&nbsp;</td>
													<td><b>Eficiente.</b></td>
												</tr>
											</table>
										</td>
									</tr>
									<tr>
										<td valign="top">
											<table>
												<tr>
													<td colspan="3" class='TextHeader'>Login</td>
												</tr>
												<tr valign="middle" runat=server id='trChave'>
													<td class="TextDefault" align="right">Chave :</td>
													<td class="TextDefault" width="110"><asp:TextBox Runat="server" ID="txtChave" MaxLength="20" width="100" />
													</td>
													<td width="100%">&nbsp;</td>
												</tr>
												<tr valign="middle">
													<td class="TextDefault" align="right">Usuário:</td>
													<td class="TextDefault"><asp:TextBox Runat="server" ID='txtUser' width="100" maxlength="20" />
													</td>
													<td>&nbsp;</td>
												</tr>
												<tr valign="middle">
													<td class="TextDefault" align="right">Senha :</td>
													<td class="TextDefault"><asp:TextBox Runat="server" ID='txtPassword' MaxLength="20" width="100" TextMode="Password" />
													</td>
													<td valign="center">
														<asp:ImageButton runat="server" id='btnOk' ImageUrl='imagens/go.gif' />
													</td>
												</tr>
												<tr>
													<td colspan="3">
														<asp:Label Runat="server" ID='lblMessage' CssClass='TextDefault'></asp:Label>
													</td>
												</tr>
											</table>
										</td>
									</tr>
								</table>
							</td>
						</tr>
					</table>
				</form>
				<!-- FIM CONTEUDO -->
			</td>
		</tr>
	</table>
	<!-- #INCLUDE FILE='inc/inc_rodape.ascx' -->
</body> 
</html> 