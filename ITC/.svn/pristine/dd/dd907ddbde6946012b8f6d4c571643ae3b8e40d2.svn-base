<script language='javascript'>
	function XMQuestion(obj, msg)
	{
		if(confirm(msg))
		{
			XMSubmit(obj);
		}
		return false;

	}
	function XMSubmit(obj)
	{
		for (var i=0;i<document.forms.length;i++)
			{
				var e = document.forms[i];
				if(e.id==obj.parentElement.id);
				{
					alert('Ok');
					e.submit();
				}
			}
	}
</script>