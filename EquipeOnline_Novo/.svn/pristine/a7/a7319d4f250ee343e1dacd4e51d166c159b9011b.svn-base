<%@ Control Language="VB" AutoEventWireup="false" CodeFile="FormatOption.ascx.vb" Inherits="Controls.FormatOption" %>
<select runat='server' id='cboFormato' style="width:150px">
    <option selected value=''>Alfanumérico (padrão)</option>
    <option value='30N'>Númerico</option>
    <option value='NN\/NN\/NNNN'>Data</option>
    <option value='NN\:NN'>Hora</option>
    <option value='NNNNN\-NNN'>CEP</option>
    <option value='AAA\-NNNN'>Placa</option>
    <option value='NNN\.NNN\.NNN\-NN'>CPF</option>
    <option value='NN\.NNN\.NNN\/NNNN\-NN'>CNPJ</option>
    <option value=''>Personalizado</option>
</select>
<asp:TextBox runat='server' ID='txtFormato' Width='160' MaxLength='30'></asp:TextBox>

