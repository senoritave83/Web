<%@ Control Language="vb" AutoEventWireup="false" Codebehind="Login.ascx.vb" Inherits="ITCOffLine.Login" TargetSchema="http://schemas.microsoft.com/intellisense/ie5" %>
	<table cellpadding="0" cellspacing="0" width="100%" align=center>
		<tr>
			<td>
				<table id="Table1" cellpadding="0" cellspacing="0" width="200" height=50 align="center" runat="server" border="1" style="border: 1px solid #6386a5;">
				<tr>
					<td>
						<table id="tblLogin" cellpadding="3" cellspacing="0" width="200" height=100 align="center" runat="server" border="0">
							<tr bgcolor="#6386a5" >
								<td colspan=2 nowrap align=center><asp:label id="Label3" CssClass="menu1" Runat="server" ForeColor=#ffffff>LOGIN DO SISTEMA</asp:label></td>
							</tr>
							<tr>
								<td nowrap><asp:label id="Label1" CssClass="menu1" Runat="server">LOGIN: </asp:label></td>
								<td><asp:textbox CssClass="f8" size="50" id="txtLogin" Runat="server" MaxLength="20"></asp:textbox><asp:requiredfieldvalidator id="RequiredFieldValidator1" Runat="server" CssClass="f8" ErrorMessage="*" ControlToValidate="txtLogin"></asp:requiredfieldvalidator></td>
							</tr>
							<tr>
								<td nowrap><asp:label CssClass="menu1" id="Label2" Runat="server">SENHA: </asp:label></td>
								<td><asp:textbox CssClass="f8" size="50" id="txtSenha" Runat="server" MaxLength="10" TextMode="Password"></asp:textbox><asp:requiredfieldvalidator id="Requiredfieldvalidator2" CssClass="f8" Runat="server" ErrorMessage="*" ControlToValidate="txtSenha"></asp:requiredfieldvalidator></td>
							</tr>
							<tr>
								<td colspan=2 valign="middle" align="center"><asp:Button runat="server" ID="btnOK" Text="OK" CssClass="Botao" CausesValidation="false" /></td>
							</tr>
							<tr>
								<td colspan="2"><asp:label CssClass="f8" id="lblMensagem" Runat="server"></asp:label></td>
							</tr>
						</table>
					</td>
				</tr>
				</table>
			</td>
		</tr>
	</table>
