<%@ Control Language="vb" AutoEventWireup="false" CodeBehind="BarraBotoes2.ascx.vb" Inherits="ITCOffLine.BarraBotoes2" %>
<table width="80%">
    <tr>
        <td align="center" width="100%" >
            <table >
                <tr>
                    <td runat="server" id="TDBotaoIncluir"><asp:Button runat="server" ID="btnIncluir" Text="Novo" CssClass="Botao" CausesValidation="false"></asp:Button></td>
                    <td runat="server" id="TDBotaoExcluir"><asp:Button runat="server" ID="btnExcluir" Text="Excluir" CssClass="Botao"></asp:Button></td>
                    <td runat="server" id="TDBotaoGravar"><asp:Button runat="server" ID="btnGravar" Text="Gravar" OnClientClick="if(confirm('Deseja realmente gravar?')==false)return false;" CssClass="Botao" ></asp:Button></td>
                    <td runat="server" id="TDBotaoVoltar"><asp:Button runat="server" ID="btn_Voltar" Text="Voltar" CssClass="Botao"  CausesValidation="false"></asp:Button></td>
                </tr>
            </table>
        </td>
    </tr>
</table>
