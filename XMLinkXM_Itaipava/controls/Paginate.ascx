<%@ Control Language="vb" AutoEventWireup="false" CodeFile="Paginate.ascx.vb" Inherits="Controls.Paginate" %>
<table width="100%" class='PaginateControl'>
	<tr class="GridItem" align="right">
		<td colspan="7">
			<asp:LinkButton ID="lnkFirst" Runat="server"><img height="17px" width="33px" src="../imagens/bt_primeiro.png"/></asp:LinkButton>
			<asp:LinkButton ID="lnkPrevious" Runat="server"><img height="17px" width="33px" src="../imagens/bt_anterior.png"/></asp:LinkButton>
			&nbsp;
			<asp:Literal runat='server' ID='ltrTexto' />
			&nbsp;
			<asp:LinkButton ID="lnkNext" Runat="server"><img height="17px" width="33px" src="../imagens/bt_proxima.png"/></asp:LinkButton>
			<asp:LinkButton ID="lnkUltima" Runat="server"><img height="17px" width="33px" src="../imagens/bt_ultimo.png"/></asp:LinkButton>
		</td>
	</tr>
</table>
